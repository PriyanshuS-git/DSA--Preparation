CODE -

//{ Driver Code Starts
#include <bits/stdc++.h>

using namespace std;

// } Driver Code Ends
//User function template for C++
class Solution{
public:	
	int search(int A[], int N){
	    //code
	    if(N==1)
	    return A[0];
	    
	    for(int i=0;i<N;i+=2)
	    {
	        if(A[i]!=A[i+1])
	        return A[i];
	    }
	    
	    return -1;
	}
};
