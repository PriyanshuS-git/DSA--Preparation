CODE -

//{ Driver Code Starts
#include <bits/stdc++.h>

using namespace std;


// } Driver Code Ends
//User function template for C++
class Solution{
public:	
	int findKRotation(int arr[], int n) {
	    // code here
	    int count = 0;
	    for(int i = 0;i< n-1;i++){
	        if(arr[i] > arr[n-1]){
	            count++;
	            
	        }
	        else{
	            break;
	        }
	    }
	    return count;
	}

};
