CODE -

//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
class Solution{
  public:
    // Function to find floor of x
    // n: size of vector
    // x: element whose floor is to find
    int findFloor(vector<long long> v, long long n, long long x){
        
        // Your code here
        
        int max=-1;
        for(int i=0;i<n;i++){
            if(v[i]<=x)
            max=i;
        }
        return max;
        
    }
};
