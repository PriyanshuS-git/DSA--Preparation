CODE -

//{ Driver Code Starts
#include<bits/stdc++.h> 
using namespace std; 

// } Driver Code Ends
//User function Template for C++
class Solution
{
public:
    long long sumOfDivisors(int N)
    {
        // Write Your Code here
        long long sum = 0;
        long long div;
        
        for(int i = 1; i<=N;i++)
        {
           div = N/i;
           sum = sum +(div*i);
        }
        return sum;
    }
};
