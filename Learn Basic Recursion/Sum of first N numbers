CODE -

//{ Driver Code Starts
// Initial template for C++
#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
// User function template for C++

class Solution {
  public:
    long long sumOfSeries(long long N) {
        // code here
        long long sum =0;
        for(long long i = 1 ;i<=N;i++)
        {
            sum = sum + i*i*i;
        }
        return sum;
        
    
    }
