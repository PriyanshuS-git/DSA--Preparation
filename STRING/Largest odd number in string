CODE -

//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
class Solution {
  public:
    string maxOdd(string s) {
        // your code here
        
        for(char i=s.length()-1;i>=0;i--){

            if(s[i]%2!=0){

             return s.substr(0,i+1);

            }

        }

        return "";
    }
};
